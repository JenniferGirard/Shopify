    "rich_text_feDUbM": {
      "type": "rich-text",
      "blocks": {
        "heading_ra8dtp": {
          "type": "heading",
          "settings": {
            "heading": "Changent has several products that will dramatically change workplaces and the way that both customers and team members view customer experience.",
            "heading_size": "h2"
          }
        },
        "text_MWtp8a": {
          "type": "text",
          "settings": {
            "text": "<p>In addition to ExperienceStream, the company has created PeerSpheres. It is a powerful human resources tool that gathers comprehensive employee experiences and provides warnings about potentially troubling workplace situations. PeerSpheres fosters positive workplace cultures and promotes employee well-being.</p>"
          }
        }
      },
